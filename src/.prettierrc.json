{
  "plugins": ["prettier-plugin-tailwindcss"],
  "arrowParens": "always",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "htmlWhitespaceSensitivity": "css",
  "jsxSingleQuote": false,
  "printWidth": 80,
  "quoteProps": "as-needed",
  "rangeStart": 0,
  "semi": true,
  "singleQuote": false,
  "singleAttributePerLine": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false
}